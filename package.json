{
  "name": "secretqr",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": false,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "start-extension": "parcel serve src/extension/index.html",
    "build-extension": "parcel build src/extension/manifest.json --config @parcel/config-webextension",
    "lint": "eslint --cache --fix --format codeframe --ext .jsx,.js src",
    "test": "jest"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@yudiel/react-qr-scanner": "^1.2.4",
    "expo": "~49.0.15",
    "expo-barcode-scanner": "^12.7.0",
    "expo-sharing": "~11.5.0",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-qrcode-styled": "^0.3.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "^14.1.0",
    "react-native-view-shot": "3.7.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@mirai/eslint": "0.1.3",
    "@parcel/config-webextension": "^2.11.0",
    "parcel": "^2.11.0",
    "process": "^0.11.10"
  },
  "alias": {
    "react-native": "react-native-web"
  },
  "targets": {
    "main": false
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "transform": {
      "^.+.jsx?$": "babel-jest"
    }
  }
}
